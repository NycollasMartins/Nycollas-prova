#include<stdio.h>
#include<math.h>

int main (){
	
	int a, b, hipotenusa, co,ca, h;
	double h4;
	float seno;
	
	printf("De o valor A e B: ");
	scanf("%d%d", &a, &b);
	co = pow(a,2);
	ca = pow(b,2);
	printf("cateto oposto eh %d e cateto adjascente eh %d", co, ca);
	h = co + ca;
	h4 = sqrt(h);
	
	printf("\nHipotenusa eh igual %.3lf", h4);
	seno = co / h;
	printf("\nSeno eh %f", seno);
}
